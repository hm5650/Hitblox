local a = game:GetService("StarterGui")
local b = game:GetService("Players")
local c = game:GetService("RunService")
local d = game:GetService("Lighting")
local e = b.LocalPlayer

local function f(g)
    local h = Instance.new("Sound")
    h.SoundId = g
    h.Parent = workspace
    h:Play()
    h.Ended:Connect(function()
        h:Destroy()
    end)
end

a:SetCore("SendNotification", {
    Title = "Loading",
    Text = "loadstring(''Made by hmmm5650'')()",
    Duration = 5
})
task.wait(4)

f("rbxassetid://5607311932")

local i = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/Arceus-X-UI-Library/main/source.lua"))()
i:SetTitle("Hitblox")
i:SetIcon("http://www.roblox.com/asset/?id=9178187770")

f("rbxassetid://195868961")

local j = 20
local k = 0.89
local l = false
local m = false
local n = {}

local function o(p)
    return p ~= e and (p.Team ~= e.Team or p.Team == nil)
end

local function q(r)
    if r then
        local s = r:FindFirstChild("Head")
        local t = r:FindFirstChild("HumanoidRootPart")

        if t then
            t.Size = Vector3.new(j, j, j)
            t.Transparency = k
            t.BrickColor = BrickColor.new("Dark red")
            t.Material = Enum.Material.Neon
            t.CanCollide = false
        end

        if s then
            s.Size = Vector3.new(j, j, j)
            s.Transparency = k
            s.BrickColor = BrickColor.new("Bright red")
            s.Material = Enum.Material.Neon
            s.CanCollide = false
            s.Massless = true
        end
    end
end

local function u(r)
    if r then
        local s = r:FindFirstChild("Head")
        local t = r:FindFirstChild("HumanoidRootPart")

        if t then
            t.Size = Vector3.new(2, 2, 1)
            t.Transparency = 1
            t.BrickColor = BrickColor.new("Medium stone grey")
            t.Material = Enum.Material.Plastic
            t.CanCollide = true
        end

        if s then
            s.Size = Vector3.new(2, 1, 1)
            s.Transparency = 1
            s.BrickColor = BrickColor.new("Medium stone grey")
            s.Material = Enum.Material.Plastic
            s.CanCollide = true
            s.Massless = false
        end
    end
end

local function v(r)
    if r then
        for _, w in ipairs(r:GetChildren()) do
            if w:IsA("BasePart") then
                w.Size = Vector3.new(1, 1, 1)
                w.Transparency = 0.8
                w.BrickColor = BrickColor.new("Dark grey")
                w.Material = Enum.Material.SmoothPlastic
                w.CanCollide = false
                w.Anchored = false
                w.Velocity = Vector3.new(0, -50, 0)
            end
        end
    end
end

local function x()
    for _, p in ipairs(b:GetPlayers()) do
        if o(p) and p.Character then
            q(p.Character)
        end
    end
end

local function y()
    for _, p in ipairs(b:GetPlayers()) do
        if p.Character then
            u(p.Character)
        end
    end
end

local z = false
local A

local function B(C)
    l = C
    f("rbxassetid://195868961")

    if l then
        x()

        if not z then
            z = true
            A = coroutine.create(function()
                while z do
                    task.wait(1)
                    x()
                end
            end)
            coroutine.resume(A)
        end
    else
        if z then
            z = false
            if A then
                coroutine.close(A)
                A = nil
            end
        end

        y()
    end
end

local function D(p)
    if n[p] then
        n[p]:Destroy()
        n[p] = nil
    end

    if not p.Character then return end
    local s = p.Character:FindFirstChild("Head")
    if not s then return end

    if o(p) then
        local E = Instance.new("BillboardGui")
        E.Name = "ESP"
        E.Adornee = s
        E.Size = UDim2.new(0, 100, 0, 50)
        E.StudsOffset = Vector3.new(0, 2, 0)
        E.AlwaysOnTop = true

        local F = Instance.new("TextLabel", E)
        F.Size = UDim2.new(1, 0, 1, 0)
        F.BackgroundTransparency = 1
        F.TextColor3 = Color3.new(1, 0, 0)
        F.TextStrokeTransparency = 0
        F.Text = p.Name
        F.TextScaled = true
        F.Font = Enum.Font.SourceSansBold

        n[p] = E
        E.Parent = s

        local function G()
            if p.Character and p.Character:FindFirstChild("HumanoidRootPart") and e.Character and e.Character:FindFirstChild("HumanoidRootPart") then
                local H = (e.Character.HumanoidRootPart.Position - p.Character.HumanoidRootPart.Position).Magnitude
                local I = 10
                local J = 100
                local K = math.clamp(J - H * 0.5, I, J)
                E.Size = UDim2.new(0, K, 0, K * 0.5)
            end
        end

        local L = tick()
        c.RenderStepped:Connect(function()
            if tick() - L >= 0.1 then
                G()
                L = tick()
            end
        end)
    end
end

local function M(C)
    m = C
    f("rbxassetid://195868961")

    if m then
        for _, p in ipairs(b:GetPlayers()) do
            if o(p) then
                D(p)
            end
        end
    else
        for _, N in pairs(n) do
            N:Destroy()
        end
        n = {}
    end
end

local function O(p)
    if o(p) then
        p.CharacterAdded:Connect(function(r)
            task.wait(1)
            if m then
                D(p)
            end
            if l then
                u(r)
            end
        end)

        p.CharacterRemoving:Connect(function()
            if p.Character then
                v(p.Character)
            end
        end)

        if p.Character and m then
            D(p)
        end
    end
end

for _, p in ipairs(b:GetPlayers()) do
    O(p)
end

b.PlayerAdded:Connect(O)
b.PlayerRemoving:Connect(function(p)
    if n[p] then
        n[p]:Destroy()
        n[p] = nil
    end
end)

i:AddButton("FBright", function()
    f("rbxassetid://6228337171")
    local d = game:GetService("Lighting")

    d.Brightness = 2
    d.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
    d.Ambient = Color3.fromRGB(255, 255, 255)
    d.TimeOfDay = "14:00:00"
    d.FogStart = 0
    d.FogEnd = 100000
    d.FogColor = Color3.fromRGB(255, 255, 255)

    game:GetService("RunService").Heartbeat:Connect(function()
        d.Brightness = 2
        d.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
        d.Ambient = Color3.fromRGB(255, 255, 255)
        d.TimeOfDay = "14:00:00"
        d.FogStart = 0
        d.FogEnd = 100000
        d.FogColor = Color3.fromRGB(255, 255, 255)
    end)
end)

i:AddButton("AntiLag", function()
    f("rbxassetid://6228337171")
    local function P()
        for _, Q in pairs(workspace:GetDescendants()) do
            if Q:IsA("Decal") and not Q:IsDescendantOf(game.Players) then
                Q:Destroy()
            end
            if Q:IsA("MeshPart") and not Q:IsDescendantOf(game.Players) then
                Q:Destroy()
            end
            if Q:IsA("SurfaceGui") then
                Q:Destroy()
            end
        end
    end
    task.wait()
    for R, S in next, (workspace:GetDescendants()) do
        if S:IsA("Part") then S.Material = Enum.Material.SmoothPlastic end
    end
end)


lib:AddToggle("Fov", function(state)
    if state then
        playSound("rbxassetid://195868961")
        local camera = game.Workspace.CurrentCamera

-- Set the camera's FOV to 120
camera.FieldOfView = 120

    else
       playSound("rbxassetid://195868961")
     local camera = game.Workspace.CurrentCamera

-- Set the camera's FOV to 120
camera.FieldOfView = 79
    end
end, false)

i:AddToggle("Expand Hitbox", function(C)
    B(C)
end, false)

i:AddToggle("ESP", function(C)
    M(C)
end, false)

i:AddButton("Aimlock", function()
    f("rbxassetid://6228337171")
    loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Aimbot-Universal-For-Mobile-and-PC-29153"))()
end)

i:AddButton("BlatantCC", function()
    f("rbxassetid://6228337171")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vxyo/RevNigga/refs/heads/main/FreeForeverBlatantCC"))()
end)

